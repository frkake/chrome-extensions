{
  "manifest_version":2,
  "name":"FuniBlock",
  "version":"1.0",
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "content_scripts":[
    {
      "matches":[
        "https://www.funimation.com/shows/*"
      ],
      "js":[
        "jquery-3.4.1.min.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
